{
    "name": "HikVideoModule",
    "id": "Hik-Video-Module",
    "version": "1.0",
    "description": "根据海康视频android原生SDK开关的插件--消耗上百根头发 ",
    "_dp_type":"nativeplugin",
    "_dp_nativeplugin":{
        "android": {
            "plugins": [
                {
                    "type": "module",
                    "name": "Hik-Video-Module",
                    "class": "com.bitmap.hikvideoplugin.HikVideoModule"
                }
            ],
            "hooksClass": "",
            "integrateType": "aar",
			"useAndroidX": true,
            "dependencies": [
               "com.blankj:utilcode:1.20.3",
			   "androidx.navigation:navigation-ui:2.2.2",
			   "androidx.appcompat:appcompat:1.1.0",
			   "androidx.navigation:navigation-fragment:2.2.2",
			   "androidx.constraintlayout:constraintlayout:1.1.3",
			   "com.squareup.okhttp3:okhttp:3.14.9",
			   "com.alibaba:fastjson:1.1.46.android",
			   "com.github.bumptech.glide:glide:4.9.0"
            ],
			"excludeDependencies": [
			   
			],
            "compileOptions": {
                "sourceCompatibility": "1.8",
                "targetCompatibility": "1.8"
            },
            "abis": [
                "armeabi-v7a","x86"
            ],
            "minSdkVersion": "24", 
            "permissions": [
                "android.permission.ACCESS_NETWORK_STATE",
                "android.permission.ACCESS_WIFI_STATE",
                "android.permission.CHANGE_WIFI_STATE",
                "android.permission.INTERNET",
                "android.permission.READ_PHONE_STATE",
                "android.permission.ACCESS_LOCATION_EXTRA_COMMANDS",
                "android.permission.CHANGE_NETWORK_STATE",
                "android.permission.CHANGE_WIFI_MULTICAST_STATE"
            ]
        }
    }
}
